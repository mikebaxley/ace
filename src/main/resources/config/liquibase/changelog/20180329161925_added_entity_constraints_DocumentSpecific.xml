<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity DocumentSpecific.
    -->
    <changeSet id="20180329161925-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="document_id"
                                 baseTableName="document_specific"
                                 constraintName="fk_document_specific_document_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_generic"/>

        <addForeignKeyConstraint baseColumnNames="document_specifics_id"
                                 baseTableName="document_specific_document_state"
                                 constraintName="fk_document_specific_document_state_document_specifics_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_specific"/>
        <addForeignKeyConstraint baseColumnNames="document_states_id"
                                 baseTableName="document_specific_document_state"
                                 constraintName="fk_document_specific_document_state_document_states_id"
                                 referencedColumnNames="id"
                                 referencedTableName="document_state"/>
        
    </changeSet>
</databaseChangeLog>
