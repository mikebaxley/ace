        <addForeignKeyConstraint baseColumnNames="ace_companies_id"
                                 baseTableName="document_generic_ace_company"
                                 constraintName="fk_document_generic_ace_company_ace_companies_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ace_company"/>
        
    </changeSet>
</databaseChangeLog>
